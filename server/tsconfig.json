{
  "compilerOptions": {
    "target": "es2016",
    "outDir": "./build",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "typeRoots": [
      "./node_modules/@types",
      "./typings"
    ],
    "noImplicitReturns": false
  },
}